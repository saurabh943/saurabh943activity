freq = float(input("Enter the frequency of electromagnetic radiation: "))

if freq < 3e9:
    print("Radio Waves")
elif 3e9 <= freq < 3e12:
    print("Microwaves")
elif 3e12 <= freq < 4.3e14:
    print("Infrared Light")
elif 4.3e14 <= freq < 7.5e14:
    print("Visible Light")
elif 7.5e14 <= freq < 3e17:
    print("Ultraviolet Light")
elif 3e17 <= freq < 3e19:
    print("X-Rays")
else:
    print("Gamma Rays")
