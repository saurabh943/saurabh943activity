def message(n):
    if len(n)<200:
        return n
    elif len(n)>=200:
        return (n[:200])
n=input()
print(message(n))

#b
m=input()
flag=True
total=0
for i in m.split():
    total+=1
    if total>30:
        flag=False
    else:
        flag=True
if flag:
    print(m)
else:
    print(None)
